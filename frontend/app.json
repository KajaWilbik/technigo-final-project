{
  "expo": {
    "name": "Planda",
    "slug": "Planda",
    "description": "Browse ideas and plan your next kids or grownup party easy with Planda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/iconPlanda.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/splashPlanda.png",
      "resizeMode": "contain",
      "backgroundColor": "#7FB77E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon-planda.png",
        "backgroundColor": "#7FB77E"
      }
    },
    "web": {
      "favicon": "./app/assets/images/iconPlanda.png"
    }
  }
}
